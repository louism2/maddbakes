<h2><%= post.title %><%= link_to 'destroy post', "/posts/#{post.id}", method: :delete, class: 'destroy_post' if signed_in? %><h2>   
<!-- 	implement post photo here  -->
<p><%= post.content %><p>
<% post.photos.each do |photo| %>
	<%= render partial: 'photos/photo', object: photo %>
<% end %>
<% post.comments.each do |user_comment| %>
	<%= render partial: 'comments/comment', object: user_comment %>
<% end %>
<%= render partial: 'comments/form', locals: {comment: comment, parent_id: post.id} %>
	

	