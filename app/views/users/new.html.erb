<%= form_for @user, html: {id: 'user_form'} do |f| %>
	<%= render partial: 'shared/error_messages', object: @user.errors.messages if @user.errors.any?%>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :email %>
	<%= f.text_field :email %>	
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation %>	
	<%= f.label :access_code %>
	<%= f.password_field :access_code %>	
	<%= f.submit "Create Account" %>
<% end %>	
	